{
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    },
    "api/players.py": {
      "runtime": "python3.9"
    },
    "api/stats.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "/var/task"
    }
  }
}