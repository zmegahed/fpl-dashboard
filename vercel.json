{
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    },
    "api/players.py": {
      "runtime": "python3.9"
    },
    "api/stats.py": {
      "runtime": "python3.9"
    }
  }
  },
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
